  "dependencies": {
    "axios": "^0.27.2",
    "convict": "^6.2.3",
    "env-ci": "^7.3.0",
    "fast-glob": "^3.2.11",
    "sharp": "^0.30.7",
    "tmp": "^0.2.1"
  },
  "devDependencies": {
    "@types/convict": "^6.1.1",
    "@babel/preset-typescript": "^7.18.6",
    "msw": "^0.44.2",
    "typescript": "^4.7.4"
