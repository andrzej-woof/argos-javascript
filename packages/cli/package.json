{
  "name": "@argos-ci/cli",
  "description": "Visual testing solution to avoid visual regression. Argos CLI is used to interact with and upload screenshots to argos-ci.com via command line.",
  "version": "0.0.0",
  "bin": {
    "argos-ci": "./bin/argos-cli.js"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "rollup -c"
  },
  "type": "module",
  "exports": {
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/argos-ci/argos-javascript.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/argos-ci/argos-javascript/tree/main/packages/core#readme",
  "bugs": {
    "url": "https://github.com/argos-ci/argos-javascript/issues"
  },
  "license": "MIT",
  "keywords": [
    "argos",
    "automation",
    "test automation",
    "testing",
    "visual testing",
    "regression",
    "visual regression"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "commander": "^9.4.0"
  },
  "devDependencies": {
    "rollup": "^2.78.0",
    "rollup-plugin-swc3": "^0.3.0"
  }
}
